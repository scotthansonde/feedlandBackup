          <outline text="Setup Docker" created="Mon, 22 Dec 2025 14:59:51 GMT">
            <outline text="Connect to github repository so we can store the image there" created="Mon, 22 Dec 2025 15:22:13 GMT">
              <outline text="Create classic personal token under developer settings" created="Mon, 22 Dec 2025 15:22:35 GMT"/>
              <outline text="echo YOUR_PAT | docker login ghcr.io -u YOUR_GITHUB_USERNAME --password-stdin " created="Mon, 22 Dec 2025 15:22:55 GMT"/>
            </outline>
            <outline text="Install node with nvm" created="Mon, 22 Dec 2025 15:11:56 GMT">
              <outline text="https://github.com/nvm-sh/nvm" created="Mon, 22 Dec 2025 15:44:28 GMT"/>
              <outline text="curl -o- https://raw.githubusercontent.com/nvm-sh/nvm/v0.40.3/install.sh | bash" created="Mon, 22 Dec 2025 15:44:30 GMT"/>
              <outline text="nvm install v24" created="Mon, 22 Dec 2025 15:45:08 GMT"/>
            </outline>
            <outline text="Create the feedland Docker image" created="Mon, 22 Dec 2025 15:47:14 GMT">
              <outline text="Install my fork of cshotton/feedlandInstall" created="Mon, 22 Dec 2025 15:41:55 GMT">
                <outline text="I've only made minimal changes" created="Mon, 22 Dec 2025 15:42:52 GMT"/>
                <outline text="git clone https://github.com/scotthansonde/feedlandDockerInstall" created="Mon, 22 Dec 2025 15:42:29 GMT"/>
              </outline>
              <outline text="Build the image and push it to GitHub" created="Mon, 22 Dec 2025 15:55:39 GMT">
                <outline text="docker buildx build --push --tag ghcr.io/scotthansonde/feedlandTest  . " created="Mon, 22 Dec 2025 15:48:10 GMT"/>
              </outline>
            </outline>
            <outline text="Personalize docker-compose.yml and run" created="Mon, 22 Dec 2025 15:47:08 GMT">
              <outline text="mkdir privateFiles " created="Mon, 22 Dec 2025 16:06:17 GMT"/>
              <outline text="docker compose up" created="Mon, 22 Dec 2025 16:03:55 GMT"/>
            </outline>
            <outline text="It works, both containers are running!" created="Mon, 22 Dec 2025 16:12:12 GMT"/>
          </outline>
          <outline text="Finish setup" created="Mon, 22 Dec 2025 16:41:07 GMT">
            <outline text="Set up database" created="Mon, 22 Dec 2025 15:41:52 GMT">
              <outline text="https://github.com/scripting/feedlandInstall/blob/main/docs/setup.sql" created="Mon, 22 Dec 2025 16:35:16 GMT"/>
            </outline>
            <outline text="Point DNS to server" created="Mon, 22 Dec 2025 16:41:22 GMT">
              <outline text="feedtest.shanson.de" created="Mon, 22 Dec 2025 16:41:29 GMT"/>
              <outline text="" created="Mon, 22 Dec 2025 16:43:14 GMT"/>
            </outline>
            <outline text="Set up config.json" created="Mon, 22 Dec 2025 16:43:21 GMT"/>
            <outline text="Install and set up caddy" created="Mon, 22 Dec 2025 16:50:33 GMT">
              <outline text="https://github.com/scripting/feedlandInstall/blob/main/docs/https.md" created="Mon, 22 Dec 2025 16:52:39 GMT"/>
            </outline>
            <outline text="Had to fiddle with docker-compose.yml to recognize config.json as a file instead of a directory." created="Mon, 22 Dec 2025 18:30:24 GMT"/>
          </outline>
          <outline text="Status: Website shows the correct head, body is empty, a error in the console. Stopping for now. ðŸ˜ƒ" created="Mon, 22 Dec 2025 18:31:05 GMT"/>
